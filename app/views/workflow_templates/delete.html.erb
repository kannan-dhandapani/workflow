<!--%= link_to("<< Back TO List", workflow_templates_path, :class => "back-link") %-->

<div class="jumbotron">

    <%= form_for(@workflow, :method => 'delete') do |f| %>
        <h3>Are you sure want to permanently delete this workflow?</h3>

        <br/>
        <p class="label label-info"><%= f.object.name %></p>
        <br/><br/>
        <%= f.submit "Back", style: "margin:5px", class: "btn pull-right" %>
        <%= f.submit "Ok", style: "margin:5px", class: "btn pull-right" %>
    <% end %>

</div>